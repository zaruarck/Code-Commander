                android:translationY="10dp"
                app:srcCompat="@drawable/plusplus" />
            <ImageView
                android:id="@+id/plusEqual"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/plusequalsnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/plusequalsnewblock" />

            <ImageView
                android:id="@+id/Minus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/minusnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/minusnewblock" />
            <ImageView
                android:id="@+id/minusEqual"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/minusequalsnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/minusequalsnewblock" />
            <ImageView
                android:id="@+id/greaterThan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/greaterthannewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/greaterthannewblock" />
            <ImageView
                android:id="@+id/greaterThanEqual"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/greaterthanequalnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/greaterthanequalnewblock" />
            <ImageView
                android:id="@+id/lowerThan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/lowerthannewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/lowerthannewblock" />
            <ImageView
                android:id="@+id/lowerThanEqual"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/lowerthanequalnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/lowerthanequalnewblock" />
            <ImageView
                android:id="@+id/Modulus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/modulusnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/modulusnewblock" />
            <ImageView
                android:id="@+id/Division"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/divisionnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/divisionnewblock" />
            <ImageView
                android:id="@+id/Multiply"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/multiplynewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/multiplynewblock" />
            <ImageView
                android:id="@+id/notEqual1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/notequalnewblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/notequalnewblock" />
            <ImageView
                android:id="@+id/Random"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/randomblock"
                android:translationX="10dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/randomblock" />







